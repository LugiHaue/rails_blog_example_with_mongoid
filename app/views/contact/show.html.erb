              <div class="row mt">
                  <div class="col-md-12">
                      <div class="content-panel">
                          <table class="table table-striped table-advance table-hover">
                            <h4><i class="fa fa-angle-right"></i> <%= @heading %> Contacts</h4>
                            <hr>
                              <thead>
                              <tr>
                                  <th><i class="fa fa-bullhorn"></i> No</th>
                                  <th><i class="fa fa-flag"></i> Name</th>
                                  <th><i class="fa fa-bookmark"></i> Subject</th>
                                  <th class="hidden-phone"><i class="fa fa-inbox"></i> Email</th>
                                  <th><i class=" fa fa-edit"></i> Status</th>
                                  <th></th>
                              </tr>
                              </thead>
                              <tbody>
                              <% @contacts.each do |contact| %>
                              <tr>
                                  <td><a href="#"><%= @no = @no + 1 %></a></td>
                                  <td><%= contact.name %> </td>
                                  <td><%= contact.subject %> </td>
                                  <td class="hidden-phone"><%= contact.email %></td>
                                  <td>
                                    <% if contact.status %>
                                      <span class="label label-success label-mini">read</span>
                                    <% else  %>
                                      <span class="label label-warning label-mini">unread</span>
                                    <% end %>
                                    </td>
                                  </td>
                                  <td> 
                                      <a href="<%= status_contact_url(contact) %>" class="btn btn-<%= contact.status ? 'success' : 'warning' %> btn-xs"><i class="fa fa-check"></i></a>
                                      <a href="<%= review_contact_url(contact) %>" class="btn btn-primary btn-xs"><i class="fa fa-eye"></i></a>
                                      <%= link_to '<i class="fa fa-trash-o"></i>'.html_safe, { action: :destroy, id: contact.id }, method: :delete, data: { confirm: 'Are you sure?'} , :class => 'btn btn-danger btn-xs'  %>
                                  </td>
                              </tr>
                              <% end %>
                              </tbody>
                          </table>
                      </div><!-- /content-panel -->
                  </div><!-- /col-md-12 -->

                  <div class="col-md-12">

                      <div class="pagination">
                        <li><%= link_to_previous_page @contacts, 'Prev' , :params => {'show' => @show} %></li>
                        <li><%= link_to_next_page @contacts, 'Next' , :params => {'show' => @show} %></li>
                      </div>

                  </div>
              </div><!-- /row -->
