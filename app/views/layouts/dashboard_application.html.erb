<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="GreenFish">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    <title>GreenFish - Dashboard Panel</title>

    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
    <!-- Bootstrap core CSS -->
    <%= stylesheet_link_tag    'dashboard/bootstrap', 'data-turbolinks-track' => true %>
    
    <!--external css-->
    <%= stylesheet_link_tag    'dashboard/font-awesome/css/font-awesome', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    'dashboard/lineicons/style', 'data-turbolinks-track' => true %>
    
    <!-- Custom styles for this template -->
    <%= stylesheet_link_tag    'dashboard/style', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    'dashboard/style-responsive', 'data-turbolinks-track' => true %>

    <%= csrf_meta_tags %>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

  </head>

  <body>

  <section id="container" >

      <!--header start-->

      <%= render partial: 'partials/dashboard/header' %>

      <!--header end-->
      
      <!--sidebar start-->

      <%= render partial: 'partials/dashboard/sidebar' %>

      <!--sidebar end-->
      
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">
            <%= yield %>
          </section>
      </section>

      <!--main content end-->
  </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <%= javascript_include_tag 'dashboard/jquery', 'data-turbolinks-track' => true %>

    <%= javascript_include_tag 'dashboard/bootstrap.min', 'data-turbolinks-track' => true %> 
    <%= javascript_include_tag 'dashboard/jquery.dcjqaccordion.2.7', 'class' => 'include' , 'data-turbolinks-track' => true %>
    
    <!--script for this page-->
    <%= javascript_include_tag "application" %>

  </body>
</html>
